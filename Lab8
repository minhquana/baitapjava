interface Car {
    void accelerate();
    void getColor();
    void getSpeed();
}
 
class Bmw implements Car{
    @Override
    public void accelerate(){
        System.out.println("...");
    }
    public void getColor(){
        System.out.println("....");
    }
    public void getSpeed(){
        System.out.println("...");
    }
}

class Civic implements Car{
    @Override
    public void accelerate(){
        System.out.println("....");
    }
    public void getColor(){
        System.out.println("......");
    }
    public void getSpeed(){
        System.out.println("....");
    }
}

class Main{
    public static void main(String[] args) {
        Bmw bmw = new Bmw();
        bmw.accelerate();
        bmw.getColor();
        bmw.getSpeed();


        Civic cv = new Civic();
        cv.accelerate();
        cv.getColor();
        cv.getSpeed();

    }
}
